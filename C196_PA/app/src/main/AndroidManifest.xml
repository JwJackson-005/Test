<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.c196_pa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.C196_PA">
        <receiver
            android:name=".UI.MyReceiver"
            android:enabled="true"
            android:exported="true"></receiver>

        <activity android:name=".UI.TermDetails" />
        <activity android:name=".UI.CourseDetails" />
        <activity android:name=".UI.AssessmentDetails" />
        <activity
            android:name=".UI.AssessmentList"
            android:label="Assessment List" />
        <activity
            android:name=".UI.CourseList"
            android:label="@string/courseList" />
        <activity
            android:name=".UI.TermList"
            android:label="@string/termList" />
        <activity
            android:name=".UI.MainActivity"
            android:label="@string/welcomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>